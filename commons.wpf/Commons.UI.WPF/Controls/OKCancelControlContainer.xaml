<Window x:Class="Commons.UI.WPF.Controls.OKCancelControlContainer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="206" Width="476">
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
		</Grid.RowDefinitions>
		<Border Grid.Row="0" Name="contentCanvas"></Border>
		<Canvas Grid.Row="1" Name="okCancelCanvas" Height="35">
			<Button Canvas.Right="6" Canvas.Top="6" Height="23" Name="cancelButton" Width="75" IsCancel="true">Cancel</Button>
			<Button Canvas.Right="87" Canvas.Top="6" Height="23" Name="okButton" Width="75" Click="okButton_Click">Ok</Button>
		</Canvas>
	</Grid>
</Window>
